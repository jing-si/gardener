<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="user">

 <select id="list" resultType="User">
 	SELECT * FROM user
 </select>
 
 <select id="list" resultType="User">
 	SELECT * FROM user 
 	WHERE userId =#{USER_ID}
 </select>

<insert id="add">
	INSERT INTO user
	VALUES (#{USER_ID},#{USER_NICK},#{USER_BIRTH},#{USER_GENDER},#{STATE_ID},#{FOREST_ID})
</insert>

<update id="update">
	UPDATE user
	SET userId=#{}, userId=#{}, userId=#{}, userId=#{}, userId=#{}, userId=#{} 
</update>
</mapper>